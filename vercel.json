{
    "version": 2,
    "builds": [
      {
        "src": "gamingform/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.10"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "gamingform/wsgi.py"
      }
    ],
    "hooks": {
      "build": {
        "command": "./start.sh"
      }
    }
  }
  